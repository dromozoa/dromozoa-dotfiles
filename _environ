echo _environ

PATH=\
$HOME/dromozoa-dotfiles/bin:\
$HOME/prefixes/dromozoa53/bin:\
$HOME/prefixes/dromozoa/bin:\
/opt/dromozoa53/bin:\
/opt/dromozoa/bin:\
/usr/local/bin:\
/usr/bin:\
/bin:\
/usr/sbin:\
/sbin

MANPATH=\
$HOME/prefixes/dromozoa53/bin:\
$HOME/prefixes/dromozoa/bin:\
/opt/dromozoa53/share/man:\
/opt/dromozoa/share/man:\
/usr/local/share/man:\
/usr/share/man

LANG=C.UTF-8

export PATH MANPATH LANG
