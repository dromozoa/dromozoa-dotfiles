case x$resource_path_v in
  x) resource_path_v=53;;
esac

PATH=\
$HOME/dromozoa-dotfiles/bin:\
$HOME/dromozoa$resource_path_v/bin:\
$HOME/dromozoa/bin:\
/opt/dromozoa$resource_path_v/bin:\
/opt/dromozoa/bin:\
/usr/local/bin:\
/usr/bin:\
/bin:\
/usr/sbin:\
/sbin

MANPATH=\
$HOME/dromozoa$resource_path_v/bin:\
$HOME/dromozoa/bin:\
/opt/dromozoa$resource_path_v/share/man:\
/opt/dromozoa/share/man:\
/usr/local/share/man:\
/usr/share/man

if (unset resource_path_v) >/dev/null 2>&1
then
  unset resource_path_v
fi

export PATH MANPATH
