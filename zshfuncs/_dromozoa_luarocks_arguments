#autoload
_dromozoa_luarocks_arguments() {
  case X$1 in
    Xbuild)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ rockspec rock name version'
      )
      ;;
    Xconfig)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ key key value key'
      )
      ;;
    Xdoc)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ argument'
      )
      ;;
    Xdownload)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ name version'
      )
      ;;
    Xhelp)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ command'
      )
      ;;
    Xinit)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ name version'
      )
      ;;
    Xinstall)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ rock name version'
      )
      ;;
    Xlint)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ rockspec'
      )
      ;;
    Xlist)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ filter'
      )
      ;;
    Xmake)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ rockspec'
      )
      ;;
    Xnew_version)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ package rockspec new_version new_url'
      )
      ;;
    Xpack)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ rockspec name version'
      )
      ;;
    Xremove)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ name version'
      )
      ;;
    Xsearch)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ name version'
      )
      ;;
    Xshow)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ argument'
      )
      ;;
    Xtest)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ rockspec args'
      )
      ;;
    Xunpack)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ rock name version'
      )
      ;;
    Xupload)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ rockspec'
      )
      ;;
    Xwhich)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ modname'
      )
      ;;
    Xwrite_rockspec)
      arguments=(
        $arguments
        ': :_luarocks_arguments __MARKER__ name version url path'
      )
      ;;
  esac
}
